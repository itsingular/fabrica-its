    <script src="~/Content/resources/scripts/axure/legacy.js"></script>
    <script src="~/Content/resources/scripts/axure/viewer.js"></script>
    <script type="text/javascript">
        $axure.utils.getTransparentGifPath = function () { return 'Content/resources/images/transparent.gif'; };
        $axure.utils.getOtherPath          = function () { return 'Content/resources/Other.html'; };
        $axure.utils.getReloadPath         = function () { return 'Content/resources/reload.html'; };

    </script>
</head>
<body bgcolor="black">
    <font color="green" size="2">@Html.ActionLink("Esqueci a Senha", "RecuperarSenha", "Home")</font><br>
        <form action="/Home/Index" method="post" autocomplete="off">
            <div id="base" class="">

                <!-- Unnamed (Image) -->
                <div id="u0" class="ax_image">
                    <img id="u0_img" class="img " src="~/images/TelaLogin.png" />
                    <!-- Unnamed () -->
                    <div id="u1" class="text">
                        <p><span></span></p>
                    </div>
                </div>

                <!-- Unnamed (Shape) -->
                <div id="u2" class="ax_paragraph">
                    <img id="u2_img" class="img " src="~/Content/resources/images/transparent.gif" />
                    <!-- Unnamed () -->
                    <div id="u3" class="text">
                        <p><span>Email:</span></p>
                    </div>
                </div>
                <!-- Unnamed (Shape) -->
                <div id="u5" class="ax_paragraph">
                    <img id="u5_img" class="img " src="~/Content/resources/images/transparent.gif" />
                    <!-- Unnamed () -->
                    <div id="u6" class="text">
                        <p><span>Senha:</span></p>
                    </div>
                </div>

                <!-- Unnamed (Text Field) -->
                <div id="u4" class="ax_text_field">
                    <input id="u4_input" type="text" name="pEmail" value="esilva@itsingular.com.br" />
                </div>

                <!-- Unnamed (Text Field) -->
                <div id="u7" class="ax_text_field">
                    <input id="u7_input" type="password" name="pSenha" value="12345" />
                </div>

                <!-- Unnamed (Shape) -->
                <div id="u8" class="ax_paragraph">
                    <img id="u8_img" class="img " src="~/Content/resources/images/transparent.gif" />
                    <!-- Unnamed () -->
                    <div id="u9" class="text" name="esquecisenha">
                        <font color="green" size="1"><a>@Html.ActionLink("Esqueci a Senha", "RecuperarSenha", "Home")</a></font><br>
                    </div>
                </div>

                <!-- Unnamed (HTML Button) -->
                <div id="u10" class="ax_html_button">
                    <input id="u10_input" type="submit" value="Acessar" with="250px" />
                </div>
                <br/><br/>
                <label id="u10ViewBag" style="color:white"><b>@ViewBag.Msg</b></label>
            </div>
        </form>
</body>
</html>

