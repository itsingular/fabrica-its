                        <li>@Html.ActionLink("Reclamante (Preposto)", "Index", "Reclamante")</li>
                        <li>@Html.ActionLink("Parte Contrária", "Index", "ParteContraria")</li>
                        <li>@Html.ActionLink("Testemunhas", "Index", "Testemunha")</li>
                        <li class="active" bgcolor=#65bf71><a href="#"></a></li>
                    </ul>
                </li>
                <li class="dropdown">
                    <a class="itslaw-item-menu" data-toggle="dropdown" href="#">Tabelas<span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Comarca", "Index", "Comarca")</li>
                        <li>@Html.ActionLink("Tipo de Ação Judicial", "Index", "TipoAcao")</li>
                        <li>@Html.ActionLink("Tipo de Área Direito", "Index", "TipoAreaDireito")</li>
                        <li>@Html.ActionLink("Tipo de Justiça", "Index", "TipoJustica")</li>
