    <!--BootBox-->
    <script src="~/Content/Scripts/bootbox.min.js"></script>



    <!--  -------------------------------------------------------------- MENU PRINCIPAL --------------------------------------------------------------------------------->
    <!--   ========================================================================================================================================================== -->
    <!--   ========================================================================================================================================================== -->
    <!-- Navigation -->
    <nav role="navigation">
        <div class="navbar-header">
            <!-- Código da Tabela -->
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td><a href="~/MenuPrincipal"><img src="/images/LogotipoBrancoMenor.png" alt="ItsLaw" title="ItsLaw" /></a></td>
                    <td width=50> </td>
                    <td>
                        <ul class="itslaw-info-empresas">Bem vindo                : @ViewBag.UsuarioNome   </ul>
                        <ul class="itslaw-info-empresas">Empresa&nbsp;&nbsp;&nbsp;: @ViewBag.UsuarioEmpresa</ul>
                    </td>
                    <td width=10> </td>
                    <td width=10> </td>
                    <td width=10> </td>
                    <td width=10> </td>
                </tr>
            </table>
        </div>
        <div id="navbarCollapse" class="collapse navbar-collapse">
            <ul class="nav navbar-nav" style="flex-direction:row;">
                <li class="dropdown">
                    <a class="itslaw-item-menu" data-toggle="dropdown" href="#">Cadastros<span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Advogados", "Index", "Advogado")</li>
                        <li>@Html.ActionLink("Escritórios", "Index", "Escritorio")</li>
                        <li>@Html.ActionLink("Empresas", "Index", "Empresa")</li>
